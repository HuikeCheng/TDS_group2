  title = {Determination of {{Acute Leukemia Lineage}} with {{New Morphologic Parameters Available}} in the {{Complete Blood Cell Count}}},
  author = {Yang, Jin Hyuk and Kim, Yonggoo and Lim, Jihyang and Kim, Myungshin and Oh, Eun-Jee and Lee, Hae-Kyung and Park, Yeon-Joon and Min, Woo Sung and Cho, Bin and Lee, Kwangyoung and Han, Kyungja},
