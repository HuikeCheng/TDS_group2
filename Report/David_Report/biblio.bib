	pages = {4305--4317},
	file = {PubMed Central Full Text PDF:C\:\\Users\\Cheng\\Zotero\\storage\\XP6CCPFR\\Wang et al. - 2019 - Prognostic role of pretreatment red blood cell dis.pdf:application/pdf},
